# API Keys Configuration
# Copy this file to .env and add your actual API keys

# Deepgram API Key for Speech-to-Text
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Vapi API Key for Text-to-Speech and Voice Interface
VAPI_API_KEY=your_vapi_api_key_here

# Anthropic Claude API Key for AI Reasoning
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Spotify API Credentials
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# News API Key
NEWS_API_KEY=your_news_api_key_here

# Reddit API Credentials
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Letta API Key for Memory and Personalization
LETTA_API_KEY=your_letta_api_key_here

# Arize API Key for Observability
ARIZE_API_KEY=your_arize_api_key_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/worldwise

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379/0

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your_secret_key_here
